    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4"
  }
}
